{
  "name": "palindrome",
  "version": "1.0.0",
  "description": "Palindrome game backend",
  "main": "dist",
  "scripts": {
    "dev": "nodemon -w src --exec \"babel-node src --presets es2015,stage-0\"",
    "build": "babel src -s -D -d dist --presets es2015,stage-0",
    "start": "node dist",
    "prestart": "npm run -s build",
    "test": "eslint src",
    "tsc": "tsc"
  },
  "eslintConfig": {
    "extends": "eslint:recommended",
    "parserOptions": {
      "ecmaVersion": 7,
      "sourceType": "module"
    },
    "env": {
      "node": true
    },
    "rules": {
      "no-console": 0,
      "no-unused-vars": 1
    }
  },
  "repository": "none",
  "author": "Nick Bafatakis ",
  "license": "MIT",
  "dependencies": {
    "@types/express": "^4.17.3",
    "body-parser": "^1.13.3",
    "collections": "^5.1.9",
    "collectionsjs": "^0.3.2",
    "compression": "^1.5.2",
    "cookie-parser": "^1.4.5",
    "cors": "^2.7.1",
    "express": "^4.13.3",
    "morgan": "^1.8.0",
    "resource-router-middleware": "^0.6.0",
    "sorted-map": "^0.1.8",
    "typescript": "^3.8.3",
    "uuid": "^7.0.2"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.9.0",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-stage-0": "^6.5.0",
    "eslint": "^3.1.1",
    "nodemon": "^1.19.4"
  }
}
